<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.HelloController">
    <children>
        <VBox layoutX="403.0" layoutY="100.0" prefHeight="200.0" prefWidth="140.0">
            <children>
                <Button fx:id="SaveAllListsButton" mnemonicParsing="false" onAction="#SaveAllListButton" text="Save All Lists" />
                <Separator prefHeight="54.0" prefWidth="140.0" />
                <Button fx:id="SaveCurrentListButton" mnemonicParsing="false" onAction="#SaveCurrentListButton" text="Save Current List" />
                <Separator prefHeight="8.0" prefWidth="140.0" />
                <Button fx:id="ImportListButton" mnemonicParsing="false" onAction="#ImportListButton" text="Import Lists" />
                <Separator opacity="0.0" prefHeight="96.0" prefWidth="140.0" />
                <Label text="Location:" />
                <TextField fx:id="LocationImportExport" onAction="#LocationImportExport" />
            </children></VBox>
        <VBox layoutX="252.0" layoutY="100.0" prefHeight="200.0" prefWidth="140.0">
            <children>
                <Button fx:id="ViewItemsButton" mnemonicParsing="false" onAction="#ViewItemsButton" text="View Items" />
                <Separator prefHeight="23.0" prefWidth="140.0" />
                <Button fx:id="DeleteListButton" mnemonicParsing="false" onAction="#DeleteListButton" text="Delete List" />
                <Separator prefHeight="23.0" prefWidth="140.0" />
                <Button fx:id="AddNewToDoButton" mnemonicParsing="false" onAction="#AddNewToDoButton" text="Add New todo" />
            </children></VBox>
        <Label layoutX="252.0" layoutY="25.0" text="To Do Application" />
        <Label layoutX="50.0" layoutY="83.0" text="ToDo List" />
        <ListView fx:id="DisplayToDoList" layoutX="30.0" layoutY="114.0" prefHeight="200.0" prefWidth="200.0" />
    </children>
</AnchorPane>
